#include <iostream>
#include <iostream>
#include <cmath>                                                                                
using namespace std;

double f(double x) {
    if (abs(x) < 3) {
        return sin(x);
    }
    else if (abs(x) >= 3 && abs(x) < 9) {
        return sqrt(pow(x,2) + 1)/sqrt(pow(x,2) + 5);
    }
    else if (abs(x) >= 9) {
        return sqrt(pow(x,2) + 1) - sqrt(pow(x,2) + 5);
    }
}
int main() {

    float a, b, h, x;
    cout << "a= ";
    cin >> a;
    cout << "b= ";
    cin >> b;
    cout << "h= ";
    cin >> h;
    cout << "x\tf(x)\n";
    for (x = a; x <= b; x += h) {
        cout << x << "\t";
        cout << f(x);
        cout << endl;
    }

}
