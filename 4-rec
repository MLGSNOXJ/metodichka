#include <iostream>

using namespace std;

int main() {
    double b1 = 100, b2,b3,n, b=0;
    cin >> n;
    cout << b1 << endl;
    for(int i = 2; i <= n; i+=2){
        b2 = b1/10;
        cout << b2 << endl;
        b3 = b2 + 10;
        if(i != n)
        cout << b3 << endl;
        b1 = b3;
        }
    

}
