
#include <iostream>
#include <iomanip>
#include <cmath>
using namespace std;
double e = -4;
double f(double x) {
    int i = 1;
    double pi;
    int b;
    int k = -1;
    pi = M_PI;
    double c = -(pi) / 2;
    b = 1;
    while (x > e) {
        c += (1 / (pow(x, i))) * k;
        return c;
        b += 2;
        k *= -1;
        i += 2;
    }
    
}
int main()
{

    double h;
    cin >> h;
    for (double i = -3; i <= -2; i += h) {
        cout << i << " " << f(i) << endl;
}
}
